ctrl + cmd - e : open -a "Microsoft Edge.app" -n
#ctrl + cmd - g : open -a "Google Chrome.app" -n
ctrl + cmd - g : ~/.config/skhd/safari
ctrl + cmd - s : ~/.config/skhd/safari

ctrl + cmd - m : open -a "Mail.app"
